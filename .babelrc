{
  "presets": [
    ["env", {
      "targets": {
        "ios": 10,
        "safari": 10,
        "edge": 14,
        "firefox": 50,
        "browsers": ["last 2 versions", "> 5%", "not ie < 12"]
      },
      "exclude": ["syntax-trailing-function-commas", "transform-exponentiation-operator"],
      "modules": false,
      "debug": false,
      "useBuiltIns": true
    }]
  ],
  "plugins": ["transform-runtime", "transform-async-to-generator"],
  "comments": false
}
